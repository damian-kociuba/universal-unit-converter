{
    "name": "Polcode/UnitConverterBundle",
    "description": "Symfony2 bundle to convert units of length, volume, temperature, mass. It's allow to easy add new units.",
    "type": "symfony-bundle",
    "keywords": ["polcode", "symfony", "bundle", "converter", "unit", "universal", "flexible"],
    "homepage": "https://github.com/damian-kociuba/universal-unit-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Kociuba",
            "homepage": "https://github.com/damian-kociuba"
        }
    ],
    "require": {
        "php": ">=5.3.1",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Polcode\\Bundle\\UnitConverterBundle\\": ""
        }
    },
    "repositories": [{
        }],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
